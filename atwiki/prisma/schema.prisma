// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User{
  id Int @id @default(autoincrement())
  name String @unique
  email String @unique
  password String
  phonenumber String? @unique
  task String?
  absent String?
  is_student Boolean
}

model Log{
  id Int @id @default(autoincrement())
  date String
  url String
  title String?
}